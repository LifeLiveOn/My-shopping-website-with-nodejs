
 <div class="container" style ="margin-top:5%"> 
   <a href='/adminmanage' style="color:black";> go back</a>
    <table class="table table-dark table-hover">
  <thead>
    <tr>
      {{!-- <th scope="col">#</th> --}}
      <th scope="col">Product name </th>
      <th scope="col">Category </th>
      <th scope="col">Price</th>
      <th scope="col">modify</th>
    </tr>
  </thead>
  <tbody>
    {{#each data}}
    <tr>
      {{!-- <th scope="row">1</th> --}}
      <td>{{this.product_name}}</td>
      <td>{{this.category}}</td>
      <td>{{this.price}}</td>
      <td><a href='edit/{{this._id}}'>update</a> &nbsp;&nbsp;<a href='delete/{{this._id}}'>delete</a></td>
    </tr>
    {{/each}}
    </tbody>
  </table>
</div>
